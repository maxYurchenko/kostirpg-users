<?xml version="1.0" encoding="UTF-8"?>
<content-type>
  <display-name>User</display-name>
  <super-type>base:structured</super-type>
  <form>
    <input name="email" type="TextLine">
      <label>Email</label>
      <occurrences minimum="1" maximum="1"/>
    </input>
    <input name="firstName" type="TextLine">
      <label>First name</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="lastName" type="TextLine">
      <label>Last name</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="city" type="TextLine">
      <label>City</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="phone" type="TextLine">
      <label>Phone</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="postalCode" type="TextLine">
      <label>Postal code</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="address" type="TextLine">
      <label>Address</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="country" type="TextLine">
      <label>Country</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="discord" type="TextLine">
      <label>Discord</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="vk" type="TextLine">
      <label>VK</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="facebook" type="TextLine">
      <label>Facebook</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="telegram" type="TextLine">
      <label>Telegram</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="kosticonnect2021" type="Long">
      <label>Kosticonnect 2021</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="userImage" type="ImageSelector">
      <label>User image</label>
      <occurrences minimum="0" maximum="1"/>
      <config>
        <allowPath>./*</allowPath>
      </config>
    </input>
    <input name="bookmarks" type="ContentSelector">
      <label>Bookmarks</label>
      <occurrences minimum="0" maximum="0"/>
      <config>
        <allowContentType>article</allowContentType>
      </config>
    </input>
    <input name="kmgPlayer" type="CheckBox">
      <label>KMG Player</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
  </form>
</content-type>